<div class="container">
		<div class="loginInfo">
			<button class="btn btn-danger" id="logout" type="submit">Logout</button>
		</div>
		<div class="bodyContent">
			<h1>Logistics Dashboard</h1>
			<div id="inputSpace">
					<form role="form" id="querydata">
							<div class="form-group" >
									<input type="date" class="form-control" id="dateBegin">
							</div>
							<div class="form-group" >
									<input type="date" class="form-control" id="dateEnd">
							</div>
							<div class="form-group" >
									<select id="siteName" class="form-control">
											{{#each sites}}
											<option value={{this.site_name}}>{{this.site_name}}</option>
											{{/each}}
									</select>
							</div>
							<button class="btn btn-primary" id="reportGen" type="submit">Submit</button>
							<button class="btn btn-primary" id="toggle" type="submit">View Top 50</button>
					</form>
			</div>
		</div>


	<div id="tableSpace" class="clearfix">  
			<div id="rowSpace">
					{{!-- <tr> --}}
							{{!-- {{#each sites}}
									<th>{{this.column}}</th>
							{{/each}}
							{{#each sites}}
