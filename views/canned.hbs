<h1>Logistics Dashboard</h1>
<div id="inputSpace">
    <form role="form" id="querydata">
        <div class="form-group" >
            <input type="date" class="form-control" id="dateBegin">
        </div>
        <div class="form-group" >
            <input type="date" class="form-control" id="dateEnd">
        </div>
        <div class="form-group" >
            <select id="siteName">
                {{#each sites}}
                <option value="{{this.site_name}}">{{this.site_name}}</option>
                {{/each}}
            </select>
        </div>
        <button id="reportGen" type="submit">Submit</button>
        <button id="limitFifty" type="submit">View Top 50</button>
    </form>
</div>



